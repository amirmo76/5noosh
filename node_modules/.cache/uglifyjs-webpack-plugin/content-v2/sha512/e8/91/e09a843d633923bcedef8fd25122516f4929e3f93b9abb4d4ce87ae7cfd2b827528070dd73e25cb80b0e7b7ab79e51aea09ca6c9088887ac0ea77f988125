px)\";\\r\\n\\t\\t\\t\\t}\\r\\n\\r\\n\\t\\t\\t\\tthis.scrollTop = function(value, force) {\\r\\n\\t\\t\\t\\t\\tif (!arguments.length) {\\r\\n\\t\\t\\t\\t\\t\\treturn -this.top();\\r\\n\\t\\t\\t\\t\\t}\\r\\n\\t\\t\\t\\t\\tthis.top(-value, force);\\r\\n\\t\\t\\t\\t};\\r\\n\\r\\n\\t\\t\\t\\tthis.scrollLeft = function(value, force) {\\r\\n\\t\\t\\t\\t\\tif (!arguments.length) {\\r\\n\\t\\t\\t\\t\\t\\treturn -this.left();\\r\\n\\t\\t\\t\\t\\t}\\r\\n\\t\\t\\t\\t\\tthis.left(-value, force);\\r\\n\\t\\t\\t\\t};\\r\\n\\r\\n\\t\\t\\t\\tthis.left = function(value, force) {\\r\\n\\t\\t\\t\\t\\tif (!arguments.length) {\\r\\n\\t\\t\\t\\t\\t\\treturn -(element.scrollLeft + offsetLeft);\\r\\n\\t\\t\\t\\t\\t}\\r\\n\\t\\t\\t\\t\\tvar dif = element.scrollLeft - prevLeft,\\r\\n\\t\\t\\t\\t\\t\\toldOffset = offsetLeft;\\r\\n\\t\\t\\t\\t\\tif ((dif > 2 || dif < -2) && !force) { //if the user interacts with the scrollbar (or something else scrolls it, like the mouse wheel), we should kill any tweens of the ScrollProxy.\\r\\n\\t\\t\\t\\t\\t\\tprevLeft = element.scrollLeft;\\r\\n\\t\\t\\t\\t\\t\\tTweenLite[\"m\" /* default */].killTweensOf(this, true, {left:1, scrollLeft:1});\\r\\n\\t\\t\\t\\t\\t\\tthis.left(-prevLeft);\\r\\n\\t\\t\\t\\t\\t\\tif (vars.onKill) {\\r\\n\\t\\t\\t\\t\\t\\t\\tvars.onKill();\\r\\n\\t\\t\\t\\t\\t\\t}\\r\\n\\t\\t\\t\\t\\t\\treturn;\\r\\n\\t\\t\\t\\t\\t}\\r\\n\\t\\t\\t\\t\\tvalue = -value; //invert because scrolling works in the opposite direction\\r\\n\\t\\t\\t\\t\\tif (value < 0) {\\r\\n\\t\\t\\t\\t\\t\\toffsetLeft = (value - 0.5) | 0;\\r\\n\\t\\t\\t\\t\\t\\tvalue = 0;\\r\\n\\t\\t\\t\\t\\t} else if (value > maxLeft) {\\r\\n\\t\\t\\t\\t\\t\\toffsetLeft = (value - maxLeft) | 0;\\r\\n\\t\\t\\t\\t\\t\\tvalue = maxLeft;\\r\\n\\t\\t\\t\\t\\t} else {\\r\\n\\t\\t\\t\\t\\t\\toffsetLeft = 0;\\r\\n\\t\\t\\t\\t\\t}\\r\\n\\t\\t\\t\\t\\tif (offsetLeft || oldOffset) {\\r\\n\\t\\t\\t\\t\\t\\tif (transformStart) {\\r\\n\\t\\t\\t\\t\\t\\t\\tif (!this._suspendTransforms) {\\r\\n\\t\\